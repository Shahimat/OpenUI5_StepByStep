<mvc:View
    controllerName="webapp.modules.main.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    id="overview">
    <Page title="{i18n>homePageTitle}">
        <headerContent>
            <Button
                icon="sap-icon://hello-world"
                press=".onOpenDialog"/>
        </headerContent>
        <content>
            <mvc:XMLView viewName="webapp.modules.main.view.HelloPanel"/>
            <mvc:XMLView viewName="webapp.modules.main.view.InvoiceList"/>
        </content>
    </Page>
</mvc:View>